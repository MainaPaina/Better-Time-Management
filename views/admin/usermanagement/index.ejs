
<table>
    <thead>
        <tr>
        <th>Username</th>
        <th>Email</th>
        <th>Role</th>
        <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
        <tr>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.role %></td>
            <td>
            <a href="/admin/usermanagement/edit/<%= user.id %>">Edit</a>
            <a href="/admin/usermanagement/delete/<%= user.id %>" onclick="return confirm('Are you sure?');">Delete</a>
            </td>
        </tr>
        <% }) %>
    </tbody>
</table>